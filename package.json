{
  "name": "nutrition-graph-starter",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev": "turbo run dev --parallel",
    "dev:api": "turbo run dev --filter=@nutrition/api",
    "dev:web": "turbo run dev --filter=@nutrition/web",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "db:build": "python3 etl/py/compile.py --in ./data/ontology --out ./data/builds/graph.dev.sqlite",
    "db:open": "sqlite3 data/builds/graph.dev.sqlite",
    "ag": "tsx scripts/aggregate.ts"
  },
  "devDependencies": {
    "turbo": "^2.1.0",
    "tsx": "^4.20.6",
    "inquirer": "^9.2.12",
    "glob": "^10.3.10"
  }
}
