{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "baseUrl": ".",
    "outDir": "dist",
    "rootDir": "src",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@lib/*": [
        "src/lib/*"
      ],
      "@nutrition/shared/*": [
        "../../packages/shared/src/*"
      ],
      "@ui/*": [
        "src/components/ui/*"
      ]
    }
  },
  "include": [
    "src"
  ]
}